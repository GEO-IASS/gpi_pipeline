<?xml version="1.0" encoding="UTF-8"?>
<!-- simplest possible DRF, just makes a simple data cube w/ no calibrations -->
<DRF ReductionType="SpectralScience" name="Quicklook Automatic Datacube Extraction">
<dataset InputDir="$(GPI_RAW_DATA_DIR)" OutputDir="$(GPI_DRP_OUTPUT_DIR)">
   <fits filename=""/>
</dataset>
<module name="Subtract Dark/Sky Background" CalibrationFile="AUTOMATIC" Save="0" gpitv="0" />
<!-- <module name="Apply Reference Pixel Correction" Save="0" gpitv="0" before_and_after='1' /> -->
<module name="Simple Destripe (hard-coded for I and T)" Save="0" gpitv="0" before_and_after='1' /> 
<module name="Load Wavelength Calibration" CalibrationFile="AUTOMATIC" />
<module name="Assemble Spectral Datacube" Save="0" />
<module name="Interpolate Wavelength Axis" Spectralchannels="37" Save="1" gpitv="5" />
</DRF>
